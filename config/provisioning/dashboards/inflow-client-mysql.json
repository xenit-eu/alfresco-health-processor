            "rawQuery": true,
            "rawSql": " SELECT\r\n    processedDateTime AS \"time\",\r\n    pdl.id as \"Id\",\r\n\t  pdl.name as \"Path\",\r\n    pdl.status as \"Status\",\r\n    pdl.parameters as \"Parameters\",\r\n    pdl.reference as \"Reference\",\r\n    c.id as \"Cycle id\",\r\n    j.name as \"Job\",\r\n    d.name as \"Destination\"\r\nFROM processedDocumentLog pdl\r\n  join cycle c on c.id =pdl.cycle_id\r\n  join job j on j.id = c.job_id\r\n  join destination d on j.destination_id = d.id\r\nWHERE\r\n  ($__timeFilter(startDateTime) or $__timeFilter(endDateTime) )\r\n  and \r\n  $__timeFilter(processedDateTime)\r\n  and\r\n    pdl.name like CONCAT(REPLACE('${path:regex}','\\\\','\\\\\\\\') , '%')\r\n  and\r\n    pdl.status in ($status)\r\n  and\r\n    j.name in ($job)\r\n  and\r\n    d.name in ($destination)\r\n  and\r\n    ('' = '${cycleId}' or FIND_IN_SET( c.id , '${cycleId}'))\r\nORDER BY processedDateTime",
