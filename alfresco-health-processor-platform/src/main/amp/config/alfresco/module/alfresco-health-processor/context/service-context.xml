<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="eu.xenit.alfresco.processor.service.healthProcessorConfiguration"
          class="eu.xenit.alfresco.processor.service.HealthProcessorConfiguration">
        <constructor-arg  name="globalProperties" ref="global-properties"/>
    </bean>
    <bean id="eu.xenit.alfresco.processor.service.processorAttributeService"
          class="eu.xenit.alfresco.processor.service.ProcessorAttributeService">
        <constructor-arg  name="attributeService" ref="attributeService"/>
        <constructor-arg  name="descriptorService" ref="descriptorComponent"/>
    </bean>
    <bean id="eu.xenit.alfresco.processor.service.cycleService"
          class="eu.xenit.alfresco.processor.service.CycleService">
        <constructor-arg name="retryingTransactionHelper" ref="retryingTransactionHelper"/>
    </bean>
</beans>