<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="healthProcessorPlatform"
          class="eu.xenit.alfresco.processor.service.ProcessorServiceImpl">
        <property name="retryingTransactionHelper" ref="retryingTransactionHelper" />
        <property name="configurationService" ref="eu.xenit.alfresco.processor.service.healthProcessorConfiguration"/>
        <property name="processorAttributeService" ref="eu.xenit.alfresco.processor.service.processorAttributeService"/>
    </bean>
</beans>
